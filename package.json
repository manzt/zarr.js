{
  "name": "zarr",
  "version": "0.1.3",
  "description": "",
  "keywords": [],
  "main": "dist/zarr.umd.js",
  "module": "dist/zarr.es5.js",
  "typings": "dist/types/zarr.d.ts",
  "files": [
    "dist"
  ],
  "author": "Guido Zuidhof <me@guido.io>",
  "repository": {
    "type": "git",
    "url": "github.com/gzuidhof/zarr.js"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:browser": "jest --config=jest-browser.config.js --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache && jest --config=jest-browser.config.js --no-cache",
    "generate-typedocs": "typedoc --out docs/typedocs --target es6 --theme minimal --readme none --mode file src",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "@types/pako": "^1.0.1",
    "@types/serve-static": "^1.13.3",
    "colors": "^1.3.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.0.0",
    "jest-dev-server": "^4.3.0",
    "jest-puppeteer": "^4.3.0",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "puppeteer": "^2.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "serve-static": "^1.14.1",
    "ts-interface-builder": "^0.2.1",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "pako": "^1.0.10",
    "ts-interface-checker": "^0.1.9"
  }
}
